{
  "exclude_extensions": [
    ".png", ".jpg", ".jpeg", ".gif", ".bmp", ".ico",".svg",
    ".mp4", ".avi", ".mov", ".mp3", ".wav",
    ".pdf", ".doc", ".docx", ".xls", ".xlsx", ".ppt", ".pptx",
    ".zip", ".tar", ".gz", ".rar", ".7z",
    ".exe", ".dll", ".so", ".dylib",
    ".ttf", ".woff", ".woff2",
    ".lock", ".sum", ".mod",
    ".md"
  ],
  "ignore_patterns": ["node_modules",".yarn",
   ".git", "__pycache__", 
   ".vscode", ".idea", 
   "dist", "build", "target", "out", "coverage", "logs"],
  "max_file_size_mb": 100,
  "min_variable_name_length": 1,
  "max_line_length": 1000,
  "output_file": "output.json",
  "verbose": false
}
