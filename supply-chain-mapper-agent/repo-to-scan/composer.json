{
    "name": "example/web-app",
    "description": "A sample web application",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "symfony/framework-bundle": "^6.0",
        "doctrine/orm": "^2.11",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
            "chmod -R 755 var/"
        ]
    }
}